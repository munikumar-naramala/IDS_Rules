# Windows Applocker rules.
#
# Eventlog to syslog service. This is what we primarily use.
# http://code.google.com/p/eventlog-to-syslog/
#
# Rules converted from https://github.com/tcw3bb/ISC_Posts/blob/master/OSSEC_AppLocker_Local_Rule.xml

# Champ Clark (08/19/2014)

alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Allowed program to execute"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id:8003; classtype: suspicious-command; program: AppLocker; sid:5008410; metadata: old_sid 5002090; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Application blocked"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8004; classtype: suspicious-command; program: AppLocker; sid:5008411; metadata: old_sid 5002091; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Allowed an MSI or script to execute"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8005; classtype: suspicious-command; program: AppLocker; sid:5008412; metadata: old_sid 5002092; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Allowed MSI/Script, but would have blocked"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8006; classtype: suspicious-command; program: AppLocker; sid:5008413; metadata: old_sid 5002093; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Prevent MSI/Script to execute"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8007; classtype: suspicious-command; program: AppLocker; sid:5008414; metadata: old_sid 5002094; rev:3;)

alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Package application allowed"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8020; classtype: suspicious-command; program: AppLocker; sid:5008415; metadata: old_sid 5002095; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Package application audited"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8021; classtype: suspicious-command; program: AppLocker; sid:5008416; metadata: old_sid 5002096; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Package application disabled"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8022; classtype: suspicious-command; program: AppLocker; sid:5008417; metadata: old_sid 5002097; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Package application installation allowed"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8023; classtype: suspicious-command; program: AppLocker; sid:5008418; metadata: old_sid 5002098; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Package application installation audited"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8024; classtype: suspicious-command; program: AppLocker; sid:5008419; metadata: old_sid 5002099; rev:3;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-APPLOCKER] Package application installation disabled"; json_map:"event_id",".EventID"; json_map:"message",".RenderedDescription"; event_id: 8025; classtype: suspicious-command; program: AppLocker; sid:5008420; metadata: old_sid 5002100; rev:3;)


